{
    "env": {
    "browser": true,
    "es2021": true,
    "jest/globals": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
    },
    "plugins": [
    "jest"
    ],
    "rules": {
    "array-callback-return": "warn",
    "jest/no-disabled-tests": "warn",
    "jest/no-focused-tests": "error",
    "jest/no-identical-title": "error",
    "jest/prefer-to-have-length": "warn",
    "jest/valid-expect": "error",
    "no-unused-vars": "warn"
    }
}
